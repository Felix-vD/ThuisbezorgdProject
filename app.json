    "ios": {
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app requires access to your location to track movement.",
        "NSLocationAlwaysUsageDescription": "This app requires access to your location even when running in the background.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "We use your location in the background to track deliveries.",
        "UIBackgroundModes": [
          "location",
          "fetch"
        ]
      },
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.stickersmash",
      "associatedDomains": [
        "applinks:your-supabase-url.supabase.co"
      ]
    },
    "android": {
      "package": "com.felix.deliverydriver",
      "versionCode": 1,
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "https",
              "host": "your-supabase-url.supabase.co",
              "pathPrefix": "/auth/callback"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
